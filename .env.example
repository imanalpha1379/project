# Binance API (Public endpoints - no key required for basic data)
VITE_BINANCE_API_URL=https://api.binance.com/api/v3
VITE_BINANCE_WS_URL=wss://stream.binance.com:9443/ws

# OpenSea API
VITE_OPENSEA_API_URL=https://api.opensea.io/api/v2
VITE_OPENSEA_API_KEY=your_opensea_api_key_here

# CoinGecko API (Optional - for additional market data)
VITE_COINGECKO_API_URL=https://api.coingecko.com/api/v3
VITE_COINGECKO_API_KEY=your_coingecko_api_key_here

# Application Settings
VITE_APP_NAME=PimcoAI
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_REAL_TRADING=false
VITE_ENABLE_NFT_TRADING=false

# Analytics (Optional)
VITE_ANALYTICS_ID=your_analytics_id_here

# Sentry (Optional - for error tracking)
VITE_SENTRY_DSN=your_sentry_dsn_here